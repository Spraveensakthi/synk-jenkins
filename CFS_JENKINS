@Library('jenkinssharedlibrary@main') _
pipeline {
    agent any
    parameters {
		string(name: 'imageName', defaultValue: 'name of the image', description: 'Name of the image to be built and deployed')
		string(name: 'branchName', defaultValue: 'sprinting', description: 'value qa will deploy on qa cluster and value starting with sprint will deploy on dev cluster')
	}

    stages {
        stage("Starting Process"){
            steps {
                script {
			GKE_CI_ImageBuilding{
			imageName = "${imageName}"
		 	branchName = "${branchName}"
            }
          }
        }
      }
    }
  }
